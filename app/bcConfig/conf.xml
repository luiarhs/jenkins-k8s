		<timeOut>20000</timeOut>
		<conectionRetries>3</conectionRetries>
	</promo>
	<terminals>
		<terminal idClones="1-999">
			<signOff>true</signOff>
			<multipleSession>false</multipleSession>
			<initialStatus>closed</initialStatus>
